<div style="width: 100%; height: 100%; overflow: auto;">
  <div class="wrapper-image-square">
    <%= image_tag "typewriter-01.svg"%>
  </div>
  <div style="position: relative; padding: 20px;">
    <%if is_ceo?(current_user.id, @prototype.course.id) %>
      <%= link_to( "#modal_edit_product_prototype", class:"btn-floating blue", style:"position: absolute; top: -20px; right: 5px;") do %>
        <i class="material-icons">mode_edit</i> 
      <% end %>
    <%end%>
    <div style="margin-bottom: : 10px;">
      <span style="font-weight: bold; color: #424242; font-size: 28px;"><%= @prototype.name%></span>
    </div>
    <div>
      <%= link_to( course_path @prototype.course.id ) do %>
        <span><%= @prototype.course.name %></span>
      <%end%>
    </div>
    <div class="">
      <%= @prototype.description %>
    </div>
  </div>
</div>
